rotate_ccw  =  Tourner de 90° dans le sens antihoraire
rotate_180  =  Une rotation de 180º
resize  =  Redimensionner
resize_canvas  =  Taille de la toile...
  
[menu_transform_status_tips]  
flip_hor  =  Retourner horizontalement l'image ou une partie de celui-ci
flip_ver  =  Retourner verticalement l'image ou une partie de celui-ci
rotate_cw  =  Tournez de 90 ° dans le sens horaire l'image ou une partie de celui-ci
rotate_ccw  =  Tourner de 90 ° dans le sens antihoraire de l'image ou une partie de celui-ci
rotate_180  =  Une rotation de 180 º de l'image ou une partie de celui-ci
resize  =  Redimensionner l'image ou une partie de celui-ci
resize_canvas  =  Redimensionner la toile
  
[menu_help_labels]  
